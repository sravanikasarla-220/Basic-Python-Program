arr=[1,2,3,4,5]
target=int(input("enter the target element"))
def binary_search(arr,target):
    lb=0
    ub=len(arr)-1
    while (lb<=ub):
        mid=(lb+ub)//2
        if(arr[mid]==target):
            return mid
        elif(arr[mid]<target):
            lb=mid+1
        else:
            ub=mid-1
    return -1
result=binary_search(arr,target)
if(result!=-1):
    print("element is found at index:",result)
else:
    print("element is not found")
