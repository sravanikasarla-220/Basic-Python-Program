day=int(input("enter the number"))
if(day==1):
    print("sunday",day)
elif(day==2):
    print("monday",day)
elif(day==3):
    print("tuesday",day)
elif(day==4):
    print("wednesday",day)
elif(day==5):
    print("thursday",day)
elif(day==6):
    print("friday",day)
elif(day==7):
    print("saturday",day)
else:
    print("error")
